{
  "name": "@yoonhaemin-lib/swagger-generator",
  "version": "2.0.2",
  "description": "Generate TypeScript API clients and Zod schemas from Swagger/OpenAPI specifications with TanStack Query support",
  "keywords": [],
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Yoon-Hae-Min/lib",
    "directory": "packages/swagger-generator"
  },
  "private": false,
  "license": "MIT",
  "files": [
    "bin",
    "lib",
    "templates"
  ],
  "bin": {
    "swagger-generator": "bin/run"
  },
  "scripts": {
    "lint": "eslint .",
    "tsc": "tsc --noEmit",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "build": "rollup --config",
    "build:prod": "pnpm lint && pnpm test && pnpm build"
  },
  "publishConfig": {
    "access": "public"
  },
  "types": "lib/index.d.ts",
  "main": "lib/index.cjs",
  "module": "lib/index.js",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs",
      "types": "./lib/index.d.ts"
    }
  },
  "dependencies": {
    "prettier": "^3.4.2",
    "swagger-typescript-api": "^13.2.16",
    "ts-to-zod": "^3.13.0",
    "yargs": "^17.7.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@babel/preset-react": "^7.25.7",
    "@babel/preset-typescript": "^7.25.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "@types/yargs": "^17.0.33",
    "@yoonhaemin-lib/eslint-config": "workspace:*",
    "@yoonhaemin-lib/typescript-config": "workspace:*",
    "babel-jest": "^29.7.0",
    "eslint": "^8",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "react": "^18",
    "react-dom": "^18",
    "rollup": "^4.24.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-dts": "^6.1.1",
    "typescript": "^5"
  }
}
