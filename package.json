{
  "name": "yoonhaemin-lib",
  "private": true,
  "scripts": {
    "build": "turbo build --filter=!@yoonhaemin-lib/template",
    "build:prod": "turbo build:prod --filter=!@yoonhaemin-lib/template",
    "dev": "turbo dev --filter=!@yoonhaemin-lib/template",
    "lint": "turbo lint --filter=!@yoonhaemin-lib/template",
    "test": "turbo test --filter=!@yoonhaemin-lib/template",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ci:publish": "changeset publish",
    "gen-package": "turbo gen workspace --copy @yoonhaemin-lib/template --destination packages --type package"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@turbo/gen": "^2.6.3",
    "prettier": "^3.2.5",
    "turbo": "^2.6.3",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@10.26.0",
  "engines": {
    "node": ">=18"
  }
}
