{
  "name": "haemin-lib",
  "private": true,
  "scripts": {
    "build": "turbo build --filter=!@repo/template",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test --filter=!@repo/template",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "ci:publish": "changeset publish",
    "gen-package": "turbo gen workspace --copy @repo/template --destination packages --type package"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@turbo/gen": "^1.12.4",
    "prettier": "^3.2.5",
    "turbo": "^2.1.3",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}
